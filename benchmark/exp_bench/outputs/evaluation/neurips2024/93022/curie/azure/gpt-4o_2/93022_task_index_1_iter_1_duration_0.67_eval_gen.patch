+import numpy as np
+import argparse
+from pyemma.coordinates import tica
+
+parser = argparse.ArgumentParser()
+parser.add_argument('--input_file', type=str, required=True)
+parser.add_argument('--output_dir', type=str, required=True)
+args = parser.parse_args()
+
+# Load torsion angles
+torsion_angles = np.load(args.input_file, allow_pickle=True).item()
+
+# Perform TICA
+tica_models = {}
+for name, angles in torsion_angles.items():
+    tica_model = tica(angles, lag=10)
+    tica_models[name] = tica_model.get_output()
+
+# Save TICA components
+os.makedirs(args.output_dir, exist_ok=True)
+np.save(os.path.join(args.output_dir, 'tica_components.npy'), tica_models)
