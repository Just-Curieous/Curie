{
  "user_message_filters": [
    "💰 Round Cost:",
    "Empty message.",
    "Openhands results:",            
    "🐚 Output:",                      
    "The command completed",       
    "Current working directory:",     
    "Command finished with exit code",
    "Event value:",                  
    "Trigger Coding Agent",           
    "Error copying files:",
    "🏦 ✅ number of saved messages:",
    "🏦 👷 number of saved messages:"           
  ],

    "action_patterns": [
    { "pattern": "^Running command:",        "action": "running command" },
    { "pattern": "^🐚 Running command:",      "action": "running command" },
    { "pattern": "Created 📁 /workspace",     "action": "creating workspace" },
    { "pattern": "Plan is:",                  "action": "creating plan" },
    { "pattern": "\\$\\$\\$\\$ Total cost",   "action": "summarizing costs" },
    { "pattern": "^Tool calls:",              "action": "tool call" },
    { "pattern": "^Message:",                 "action": "tool message" },
    { "pattern": "^Event value:",             "action": "event value" },
    { "pattern": "^🕒 This may take awhile",    "action": "delay notice" },
    { "pattern": "Error copying files:",      "action": "error" },
    { "pattern": "^💸",                        "action": "cost summary" },
    { "pattern": "^🐚",                        "action": "shell output" },
    { "pattern": "^Debugging[: ]",           "action": "debugging"           },
    { "pattern": "Test (passed|failed)",     "action": "testing"             },
    { "pattern": "^Executing task:",         "action": "executing"           },
    { "pattern": "(Designing|Created) experiment", "action": "design experiment" },
    { "pattern": "Coding[: ]",               "action": "coding"              },
    { "pattern": "(Analyzing|Analysis):",    "action": "analyzing"           },
    { "pattern": "(Loading|Loaded) data",    "action": "loading data"        },
    { "pattern": "Connecting to",            "action": "network operation"   },
    { "pattern": "Retrying",                 "action": "retry attempt"       },
    { "pattern": "✅ Completed",             "action": "completion"          },
    { "pattern": "^=+ Global Step \\d+ =+$",  "action": "global step" },
    { "pattern": "Preparing environment", "action": "environment setup" },
    { "pattern": "Full response from", "action": "getting response" },
    { "pattern": "<<<<<<<< Scheduling ([^ ]+) ⏩ ([^ ]+) >>>>>>>>", "action": "change agent" },
    { "pattern": "Response: ", "action": "getting response" },
    { "pattern": "Messages TO SUPERVISOR 👑", "action": "Messaging exp-supervisor.txt to the agent" },
    { "pattern": "Messages TO CONTROL_WORKER_0 👷", "action": "Messaging controlled-worker.txt to the agent" },
    { "pattern": "Messages TO LLM_VERIFIER ✅", "action": "Messaging llm-verifier.txt to the agent" },
    { "pattern": "Messages TO ANALYZER ✅", "action": "Messaging exp-concluder.txt to the agent" },
    { "pattern": "Messages TO CONCLUDER ✅", "action": "Messaging exp-concluder.txt to the agent" }
    ],


  "agent_markers": [
    { "pattern": "<><><><><> 👑 ([A-Z0-9_]+) 👑 <><><><><>", "group": 1 },
    { "pattern": "<><><><><> 👷 ([A-Z0-9_]+) 👷 <><><><><>", "group": 1 },
    { "pattern": "<><><><><> ✅ ([A-Z0-9_]+) ✅ <><><><><>", "group": 1 }
  ],

  "colors": {
    "grey": "\\u001b[38;5;240m",
    "reset": "\\u001b[0m"
  },

  "formatters": {
    "console": {
      "format": "{GREY}{asctime}{RESET} - {name} - {GREY}{filename}{RESET} - {levelname} - {message}",
      "datefmt": "%Y-%m-%d %H:%M:%S"
    },
    "verbose": {
      "format": "%(asctime)s - %(name)s - %(filename)s:%(lineno)d - %(levelname)s - %(message)s",
      "datefmt": "%Y-%m-%d %H:%M:%S"
    },
    "user": {
      "format": "🤖 {agent}\n📝 Action: {action}\n🔍 {methodology}\n{sep}",
      "change_agent_format": "Changing agent: {from_agent} → {to_agent}"
    }
  }
}
